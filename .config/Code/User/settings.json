// Place your settings in this file to overwrite the default settings
{
    "editor.fontFamily": "'Fira Code'",
    "editor.fontSize": 14,
    "editor.fontLigatures": true,
    "window.restoreWindows": "all",
    "clang.executable": "clang-4.0",
    "cmake-tools-helper.cmake_download_path": "/home/marci/.vscode/extensions/maddouri.cmake-tools-helper-0.1.1/cmake_download",
    "editor.renderWhitespace": "none",
    "extensions.ignoreRecommendations": false,
    "window.zoomLevel": 0,
    "editor.wordWrap": "on",
    "[cpp]": {},
    "editor.multiCursorModifier": "ctrlCmd",
    "explorer.confirmDelete": false,
    "editor.minimap.enabled": false,
    "gitlens.codeLens.recentChange.enabled": false,
    "clang-format.language.cpp.enable": true,
    "C_Cpp.errorSquiggles": "Disabled",
    "C_Cpp.exclusionPolicy": "checkFilesAndFolders",
    "C_Cpp.workspaceParsingPriority": "highest",
    "C_Cpp.clang_format_sortIncludes": true,
    "C_Cpp.intelliSenseEngineFallback": "Disabled",
    "c-cpp-flylint.flexelint.enable": false,
    "lldb.executable": "lldb-4.0",
    "c-cpp-flylint.cppcheck.force": true,
    "c-cpp-flylint.standard": [
        "c11",
        "c++11"
    ],
    "gitlens.keymap": "alternate",
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true
    },
    "zenMode.centerLayout": false,
    "zenMode.hideTabs": false,
    "gitlens.historyExplorer.enabled": true
}
